{
  "extends": [
    "@weahead/eslint-config-react"
  ],
  "rules": {
    "indent": [
      2,
      2
    ],
    "semi": [
      "error",
      "always"
    ],
    "react/jsx-indent": [
      "error",
      2
    ],
    "react/jsx-pascal-case": [
      2,
      {
        "ignore": "Styled.hr"
      }
    ]
  }
}